<?xml version="1.0" encoding="utf-8" ?>
<vbox id="list-pts-container" width="100%" height="100%">
    <namesortselector id="list-pts-sortby" width="100%" />

    <scrollview width="100%" height="100%" style="backgroundColor: #FFFFFF;">
        <texttable id="list-pts-table" width="100%" columns="4">
            <texttablehead width="10%"><text text="Name" width="100%" /></texttablehead>
            <texttablehead width="15%"><text text="" width="100%" /></texttablehead>
            <texttablehead width="30%"><text text="Labs" width="100%" /></texttablehead>
            <texttablehead width="45%"><text text="Office Hours" width="100%" /></texttablehead>
        </texttable>
    </scrollview>
    
    <hbox width="100%">
        <button id="list-pts-export-btn" width="100%" text="Export to CSV" />
    </hbox>

</vbox>